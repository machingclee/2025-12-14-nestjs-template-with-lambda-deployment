        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "jest": {
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "ts",
            "tsx"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
