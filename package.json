{
    "name": "file-generation",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "gen:entity": "ts-node scripts/generate-entities.ts --table",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.825.0",
        "@aws-sdk/lib-storage": "^3.826.0",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.1",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/typeorm": "^11.0.0",
        "@react-pdf/renderer": "^3.4.4",
        "@types/aws-lambda": "^8.10.149",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/pg": "^8.16.0",
        "aws-serverless-express": "^3.4.0",
        "axios": "^1.9.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "dayjs": "^1.11.13",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
        "nest-commander": "^3.17.0",
        "normalizr": "^3.6.2",
        "pg": "^8.16.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "sharp": "^0.33.5",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.24",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.29",
        "@types/package-name-regex": "^3.1.0",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@types/supertest": "^6.0.2",
        "esbuild": "0.25.5",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^16.1.0",
        "prettier": "^3.5.3",
        "serverless": "3.38.0",
        "serverless-esbuild": "^1.55.0",
        "serverless-plugin-common-excludes": "^4.0.0",
        "serverless-plugin-typescript": "^2.1.5",
        "serverless-plugin-typescript-express": "^1.0.8",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
